{
  "compilerOptions": {
    "module": "Node16",
    "target": "es2022",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": "src"
  }
}
